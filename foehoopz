        plr.Character["Torso"]:AddPropertyEmulator("Size") -- fake size
        plr.Character["Torso"]:AddPropertyEmulator("Size") -- fake size
        
        for i,v in pairs(getconnections(plr.Character["Torso"]:GetPropertyChangedSignal("Size"))) do
            v.Function = error
            v:Disable() -- disable property changed
        end
        for i,v in pairs(getconnections(plr.Character["Torso"]:GetPropertyChangedSignal("Size"))) do
            v.Function = error
            v:Disable() -- disable property changed
        end

        plr.Character["Torso"].Size = Vector3.new(7, 5, 2.5)
        plr.Character["Torso"].Size = Vector3.new(7, 5, 2.5)
